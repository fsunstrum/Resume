      \strng{namehash}{40d092299212f7a682fd54e5ddc7f394}
      \strng{fullhash}{43df1227e34ae125e3cab63a66b2b58c}
      \strng{bibnamehash}{43df1227e34ae125e3cab63a66b2b58c}
      \strng{authorbibnamehash}{43df1227e34ae125e3cab63a66b2b58c}
      \strng{authornamehash}{40d092299212f7a682fd54e5ddc7f394}
