      \field{year}{2024}
      \field{pages}{766\bibrangedash 785}
      \range{pages}{20}
      \verb{doi}
      \verb https://doi.org/10.1111/tpj.16528
      \endverb
      \verb{eprint}
      \verb https://onlinelibrary.wiley.com/doi/pdf/10.1111/tpj.16528
      \endverb
